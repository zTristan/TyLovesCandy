<% if not @upload or @upload.new_record? %>
  alert("Failed to upload file: <%= j @upload.errors.full_messages.join(', ').html_safe %>");
<% else %>
  <% if @upload._type == "Video" %>
    $("#videos").prepend("<%= j quick_edit(@upload, @categories) %>");
  <% else %>
    $("#photos").prepend("<%= j quick_edit(@upload, @categories) %>");
  <% end %>
<% end %>
