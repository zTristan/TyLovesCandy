<div class="video col-md-4">
  <div class="span12 text-center page-header">
    <h2><%= video.title %><small><%= video.description %></small></h2>
    <div data-swf="/flowplayer.swf" class="flowplayer minimalist <%= size_class %>">
      <video poster="<%= video.thumbnail.url %>">
        <source type="<%= video.upload_content_type %>" src="<%= video.upload.url %>"/>
      </video>
    </div>
  </div>
</div>
